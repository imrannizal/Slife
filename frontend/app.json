{
  "expo": {
    "scheme": "SlifeOfLife",
    "name": "Slife",
    "slug": "slife",
    "originalFullName": "@imrannizal/Slife",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bird-slife.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/bird-slife.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bird-slife.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.imrannizal.Slife",
      "GoogleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      "expo-build-properties",
      "@react-native-firebase/app"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2dd8334e-ec3b-4607-b2a4-41f160cefb28"
      }
    },
    "owner": "imrannizal"
  }
}
